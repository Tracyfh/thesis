@Article{Montgomery1988,
  author        = {Montgomery, David R. and Dietrich, William E.},
  title         = {{Where do channels begin?}},
  journal       = {Nature},
  year          = {1988},
  volume        = {336},
  number        = {6196},
  pages         = {232--234},
  issn          = {0028-0836},
  __markedentry = {[Gennadii:6]},
  abstract      = {The closer channels begin to drainage divides, the greater will be the number of channels that occupy a unit area, and consequently the more finely dissected will be the landscape. Hence, a key component of channel network growth and landscape evolution theories1-7, as well as models for topographically controlled catch-ment runoff8, should be the prediction of where channels begin. Little field data exist, however, either on channel head locations9-14 or on what processes act to initiate and maintain a channel14-17. Here we report observations from several soil-mantled regions of Oregon and California, which show that the source area above the channel head decreases with increasing local valley gradient for slopes ranging from 5 to 45 degrees. Our results support a predicted relationship between source area and slope for steep humid landscapes where channel initiation is by landsliding, but they contradict theoretical predictions for channel initiation by overland flow in gentle valleys. Our data also suggest that, for the same gradient, drier regions tend to have larger source areas.},
  doi           = {10.1038/336232a0},
  file          = {:C$\backslash$:/Users/Gennadii/AppData/Local/Mendeley Ltd./Mendeley Desktop/Downloaded/Montgomery, Dietrich - 1988 - Where do channels begin.pdf:pdf},
  groups        = {PAPER HAND},
  isbn          = {0028-0836},
}

