@Article{Wood2011,
  author  = {Wood, Eric F. and Roundy, Joshua K. and Troy, Tara J. and van Beek, L. P. H. and Bierkens, Marc F. P. and Blyth, Eleanor and de Roo, Ad and D{\"{o}}ll, Petra and Ek, Mike and Famiglietti, James and Gochis, David and van de Giesen, Nick and Houser, Paul and Jaff{\'{e}}, Peter R. and Kollet, Stefan and Lehner, Bernhard and Lettenmaier, Dennis P. and Peters-Lidard, Christa and Sivapalan, Murugesu and Sheffield, Justin and Wade, Andrew and Whitehead, Paul},
